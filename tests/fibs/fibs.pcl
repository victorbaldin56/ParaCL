/**
 * Prints fibonacci numbers til the n'th.
 */

n = 0;
a = 0;
b = 1;
x = ?; // take x from the standard input

while(n < x) {
  n = n + 1;

  if (n == 1)
    print a;

  if (n == 2)
    print b;

  if (n > 2) {
    tmp = b;
    b = a + b; // calculate fibonacci number
    a = tmp;
    print b;
  }
}
